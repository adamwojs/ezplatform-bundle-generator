{
  "name": "adamwojs/ezplatform-bundle-generator",
  "license": "GPL-2.0-only",
  "description": "Symfony Bundle generator for eZ Platform based projects",
  "keywords": [
    "ezplatform", "symfony"
  ],
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "symfony/console": "^4.3",
    "symfony/filesystem": "^4.3"
  },
  "require-dev": {
    "composer/composer": "^1.0@dev",
    "phpunit/phpunit": "^8.2",
    "ezsystems/ezplatform-code-style": "^0.1.0"
  },
  "bin": ["bin/ezplatform-bundle-generator"],
  "autoload": {
    "psr-4": {
      "AdamWojs\\EzPlatformBundleGenerator\\": "src"
    }
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
